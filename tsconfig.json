{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es2017",  // or higher
    "module": "esnext",   // or "es2022"
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
